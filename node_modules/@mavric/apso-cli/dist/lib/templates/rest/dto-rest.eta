import { ApiProperty } from '@nestjs/swagger';
<%~ includeFile('./header.eta') %>

<% if (it.importEnums) { %>
import * as enums from '../../enums';
<% } %>

export class <%= it.name %>Create {
<% it.columns.forEach((field) => { %>
    @ApiProperty()
  <%if (field.dataType.includes("Enum")) {%>
    <%= field.name %>: enums.<%= field.dataType %>;
  <%} else if (field.type === 'json' || field.type === 'json-plain') {%>
    <%= field.name %>: JSON;
  <%} else if (field.type === 'timestamp' || field.type === 'timestamptz') {%>
    <%= field.name %>: Date;
  <%} else {%>
    <%= field.name %>: <%= field.dataType %>;
  <% } %>
<% }) %>
};

export class <%= it.name %> {
<% if (it.addDefaultPKProperty) { %>
    @ApiProperty()
    id: number;
<% } %>
<% it.columns.forEach((field) => { %>
    @ApiProperty()
  <%if (field.dataType.includes("Enum")) {%>
    <%= field.name %>: enums.<%= field.dataType %>;
  <%} else if (field.type === 'json' || field.type === 'json-plain') {%>
    <%= field.name %>: JSON;
  <%} else if (field.type === 'timestamp' || field.type === 'timestamptz') {%>
    <%= field.name %>: Date;
  <%} else {%>
    <%= field.name %>: <%= field.dataType %>;
  <% } %>
<% }) %>
<% if (it.createdAt) { %>
    @ApiProperty()
    created_at: Date;
<% } %>
<% if (it.updatedAt) { %>
    @ApiProperty()
    updated_at: Date;
<% } %>
};